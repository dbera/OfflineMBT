grammar nl.esi.comma.project.standard.StandardProject with nl.esi.comma.types.Types

generate standardProject "http://www.esi.nl/comma/project/standard/StandardProject"

Project:
	'Project' name = ID 
	'{'
		'Generate' 'product' '{'
			taskname = ID '{'
				'product-file' product = STRING
			'}'
		'}'
	'}'
;


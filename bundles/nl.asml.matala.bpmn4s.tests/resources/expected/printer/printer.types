record UNIT {
	int	unit
}

enum ColorType { MONOCHROME COLOR }

record CorrectionItem {
    string	data
}

record CorrectionsReport {
    mixed map<int,CorrectionItem[]>	correctionsMap
    int	id
}

record FactoryCtx {
    int	id
    PrintResolution	resolution
    real	scale
    ColorType	color
}

record MeasureRequest {
    int	id
    symbolic Report	printJobReport
}

enum OperationType { PREP PRINT }

record OptimizeRequest {
    int	id
}

enum Outcome { OK NOK }

record PrintRequest {
    int	id
    PrintResolution	resolution
    real	scale
    ColorType	color
    OperationType	opType
}

enum PrintResolution { LOW MED HIGH }

record Report {
    int	id
    symbolic string	payload
    symbolic real	kpi
}

record Result {
    Outcome	verdict
}

